package unidade05;

import java.util.Locale;
import java.util.Scanner;

public class MatrizIdentidade {
	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int i;
		int j;
		int I [] [] = new int [3] [3];
		
		for (i = 0; i < 3; i ++) 
			for (j = 0; j < 3; j ++) {
				
				if (i == j) {
					I [i] [j] = 1;
				
				} else {
					I [i] [j] = 0;
				}
			
			}
		
		
		
		sc.close();
	}

}
